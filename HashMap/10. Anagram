s = "listen"
t = "silentzzz" 


from collections import defaultdict

def anagram(s, t):
    counter1 = defaultdict(int)
    for c in s:
        counter1[c] = counter1.get(c, 0) + 1
    
    counter2 = {}
    for c in t:     # â† FIXED
        counter2[c] = counter2.get(c, 0) + 1
        
    return counter1 == counter2

    

print(anagram(s, t))
    
